/* SHAPES */
.rectangle{width:200px}.circle,.rectangle{height:100px;background:red}
.circle{width:100px;border-radius:50px}
.oval{width:200px;height:100px;background:red;border-radius:100px/50px}
.triangleUp{border-bottom:100px solid red}
.triangleDown,.triangleUp{width:0;height:0;border-left:50px solid transparent;border-right:50px solid transparent}
.triangleDown{border-top:100px solid red}
.heart{position:relative;height:75px;transform:rotate(45deg)}
.heart,.heart:before{content:"";width:50px;border:none;border-radius:25px 25px 0 0;background-color:purple}
.heart:before{position:absolute;height:76px;transform:rotate(-90deg);transform-origin:50% 66.7%}

/* CUSTOM INPUT WRAPPER */
.custom-input-wrap label{display:inline-block;cursor:pointer;position:relative;padding-left:25px;margin-right:30px;margin-top:5px;font-size:13px;line-height:22px}
.custom-input-wrap label:before{content:"";display:inline-block;width:16px;height:16px;margin-right:10px;position:absolute;top:3px;left:0;width:16px;height:16px;border-radius:50%;border:1px solid silver;}
.custom-input-wrap label:after{content:"";position:absolute;display:none}
.checkbox label:before{border-radius:3px;}
.large.custom-input-wrap label{padding-left:35px;}
.large.custom-input-wrap label:before{border:2px solid silver;}

/* ROTATE & ZOOM */
.hover-zoom-effect{overflow:hidden}
.hover-zoom-effect i{-moz-transition:all .3s ease;-o-transition:all .3s ease;-webkit-transition:all .3s ease;transition:all .3s ease}
.hover-zoom-effect:hover i{opacity:.4;-moz-transform:rotate(-32deg) scale(1.4);-ms-transform:rotate(-32deg) scale(1.4);-o-transform:rotate(-32deg) scale(1.4);-webkit-transform:rotate(-32deg) scale(1.4);transform:rotate(-32deg) scale(1.4)}

/* LARGE CHECKBOX */
.big-checkbox{transform:scale(2,2);margin-left:8px;margin-right:10px}

/* FADING HR */
.hr-divider{position:relative;border-width:0 0 1px;border-style:solid;border-color:transparent;}
.hr-divider:after,.hr-divider:before{content:"";position:absolute;display:block;width:100%}
.hr-divider:before{background:silver;background:linear-gradient(90deg,hsla(0,0%,75%,0),silver,hsla(0,0%,75%,0));bottom:0;height:1px}
.hr-divider:after{background:radial-gradient(at 50% 0,rgba(74,74,74,.15),transparent 0);top:100%;height:10px}
.hr-divider-right{border-width:0 1px 1px 0;margin-bottom:0}
.hr-divider-right:before{background:silver;background:linear-gradient(180deg,hsla(0,0%,75%,0),silver,hsla(0,0%,75%,0));top:0;width:1px;height:100%;right:0}
.hr-divider-right:after{background:radial-gradient(farthest-corner at 0 50%,rgba(74,74,74,.15),transparent 40%);top:0;height:100%;width:10px;right:-10px}
.hr-divider-left{border-width:0 0 1px 1px;margin-bottom:0}
.hr-divider-left:before{background:silver;background:linear-gradient(180deg,hsla(0,0%,75%,0),silver,hsla(0,0%,75%,0));top:0;width:1px;height:100%;left:0}
.hr-divider-left:after{background:-webkit-radial-gradient(100% 50%,farthest-corner,rgba(74,74,74,.15),transparent 0);background:-moz-radial-gradient(100% 50%,farthest-corner,rgba(74,74,74,.15),transparent 40%);background:-ms-radial-gradient(100% 50%,farthest-corner,rgba(74,74,74,.15),transparent 40%);background:-o-radial-gradient(100% 50%,farthest-corner,rgba(74,74,74,.15),transparent 40%);background:radial-gradient(farthest-corner at 100% 50%,rgba(74,74,74,.15),transparent 40%);top:0;height:100%;width:10px;left:-10px}

/* IMAGE PLACEHOLDER */
img.placeholder{content:"";position:relative;color:#999;background:#ccc;display:inline-block}
.img.placeholder[title]:after{content:attr(title)}
img.placeholder[title]:after{content:attr(width) " " "\d7" " " attr(height);font-weight:bold;position:absolute;top:50%;left:0;width:100%;margin-top:-.5em;overflow:hidden;text-align:center}

/* CUSTOM CHECKBOX */
input[type='checkbox']:checked + label:after{display:block;top:3px;left:5px;box-sizing:border-box;width:6px;height:12px;transform:rotate(45deg);border-width:2px;border-style:solid;border-top:0;border-left:0}
.large input[type='checkbox'] + label:before{width:24px;height:24px;}
.large input[type='checkbox'] + label:after{padding-right:10px;}
.large input[type='checkbox'] + label{padding-top:5px;}
.large input[type='checkbox']:checked + label:after{display:block;top:1px;left:5px;box-sizing:border-box;width:1px;height:20px;transform:rotate(45deg);border-width:3px;border-style:solid;border-top:0;border-left:0}

/* CUSTOM RADIO */
input[type='radio']:checked + label:after{display:block;top:7px;left:4px;width:8px;height:8px;border-radius:50%}
.large input[type='radio'] + label:before{width:24px;height:24px;}
.large input[type='radio']:checked + label:after{display:block;top:7px;left:4px;width:16px;height:16px;border-radius:50%}

/* CUSTOM CHECKBOX */
.custom-checkbox{display:inline-block;width:50px;height:50px;cursor:pointer}
.custom-checkbox svg{cursor:pointer}
.custom-checkbox [type="checkbox"]{display:none}
.custom-checkbox [type="checkbox"] ~ svg>*{stroke:#d3d3d3;-webkit-transition:stroke .2s ease-in-out;transition:stroke .2s ease-in-out}
.custom-checkbox [type="checkbox"] ~ svg>path{stroke-width:16}
.custom-checkbox [type="checkbox"] ~ svg>path.check{stroke-dashoffset:100%;stroke-dasharray:100%,150%;-webkit-transition:stroke-dashoffset .2s ease-in-out;transition:stroke-dashoffset .2s ease-in-out}
.custom-checkbox [type="checkbox"]:checked ~ svg>*{stroke:orange}
.custom-checkbox [type="checkbox"]:checked ~ svg>path.check{stroke-dashoffset:0}
.custom-checkbox [type="checkbox"]:not(:checked) ~ svg>path.check{stroke-dashoffset:100%}
.custom-checkbox.custom-checkbox-cisco-blue [type="checkbox"]:checked ~ svg>*{stroke:#085071}
.custom-checkbox.custom-checkbox-danger [type="checkbox"]:checked ~ svg>*{stroke:#D34135}
.custom-checkbox.custom-checkbox-success [type="checkbox"]:checked ~ svg>*{stroke:#6DBF83}
.custom-checkbox.custom-checkbox-primary [type="checkbox"]:checked ~ svg>*{stroke:#3c8dbc}
.custom-checkbox.custom-checkbox-info [type="checkbox"]:checked ~ svg>*{stroke:#00c0ef}
.custom-checkbox.custom-checkbox-warning [type="checkbox"]:checked ~ svg>*{stroke:#E79F3C}

/* CUSTOM RADIO */
.custom-radio{display:inline-block;width:50px;height:50px;cursor:pointer}
.custom-radio label{cursor:pointer}
.custom-radio [type="radio"]{display:none}
.custom-radio [type="radio"] ~ svg{cursor:pointer}
.custom-radio [type="radio"] ~ svg>*{stroke:#d3d3d3;-webkit-transition:stroke .2s ease-in-out;transition:stroke .2s ease-in-out}
.custom-radio [type="radio"] ~ svg>.outer-circle{stroke-width:8}
.custom-radio [type="radio"] ~ svg>.inner-circle{-webkit-transition:stroke-width .2s ease-in-out;transition:stroke-width .2s ease-in-out;stroke-width:8}
.custom-radio [type="radio"]:checked ~ svg>*{stroke:orange}
.custom-radio [type="radio"]:checked ~ svg>.inner-circle{stroke-width:50;stroke-linecap:round}
.custom-radio [type="radio"]:not(:checked) ~ svg>.inner-circle{stroke-width:0;stroke-linecap:round}
.custom-radio.custom-radio-cisco-blue [type="radio"]:checked ~ svg>*{stroke:#085071}
.custom-radio.custom-radio-danger [type="radio"]:checked ~ svg>*{stroke:#D34135}
.custom-radio.custom-radio-success [type="radio"]:checked ~ svg>*{stroke:#6DBF83}
.custom-radio.custom-radio-primary [type="radio"]:checked ~ svg>*{stroke:#3c8dbc}
.custom-radio.custom-radio-info [type="radio"]:checked ~ svg>*{stroke:#00c0ef}
.custom-radio.custom-radio-warning [type="radio"]:checked ~ svg>*{stroke:#E79F3C}

