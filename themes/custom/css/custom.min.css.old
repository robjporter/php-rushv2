/* TESTING */
.main-container {
  float: left;
  width: 100%;
  overflow: hidden;
}



/* TO SORT */
html{-webkit-tap-highlight-color: transparent;}
.card-body{padding-bottom:20px;padding-top:20px;font-weight:bold;}
.card-title{font-weight:bold !important;}




/* STATS BAR */
.stats-list{list-style-type:none;clear:left;margin:0;padding:0;text-align:center;margin-bottom:30px}
.stats-list .stats-list-positive{color:#3adb76}
.stats-list .stats-list-negative{color:#cc4b37}
.stats-list>li{display:inline-block;margin-right:10px;padding-right:10px;border-right:1px solid #cacaca;text-align:center;font-size:1em;font-weight:700}
.stats-list>li:last-child{border:none;margin:0;padding:0}
.stats-list>li .stats-list-label{display:block;margin-top:2px;font-size:.7em;font-weight:400}

/* DATA BOX */
.data-box{min-height:160px;margin-bottom:1.5rem;text-align:center;background:#fff;border:1px solid #c2cfd6}
.data-box i{display:block;margin:-1px -1px 0;font-size:40px;line-height:90px;background:#c2cfd6}
.data-box ul{padding:10px 0;list-style:none}.data-box ul li:first-child{border-right:1px solid #a4b7c1}
.data-box ul li{display:block;float:left;width:50%;text-align:-webkit-match-parent}
.data-box ul li strong{display:block;font-size:20px;margin-top:-10px;margin-bottom:10px}
.data-box ul li span{font-size:10px;font-weight:500;color:#a4b7c1;text-transform:uppercase}

/* DATA BOX 2 */
.data-box-2{min-height:160px;margin-bottom:1.5rem;background:#fff;border:1px solid #c2cfd6}

/* MPANEL */
.m-panel{background:#fff;border-top:3px solid #d2d6de;-moz-border-radius:3px;border-radius:3px;-webkit-box-shadow:0 1px 1px rgba(0,0,0,.15);box-shadow:0 1px 1px rgba(0,0,0,.15)}
.m-panel:after,.m-panel:before{display:table;content:"";line-height:0}
.m-panel:after{clear:both}
.m-panel .panel_hd{padding:10px;border-bottom:1px solid #f4f4f4;margin-bottom:1px}
.m-panel .panel_bd{padding:10px}
.m-panel .panel_ft{padding:10px;border-top:1px solid #f4f4f4}
.m-panel.m-panel-danger{border-top:3px solid #D34135;}
.m-panel.m-panel-success{border-top:3px solid #6DBF83;}
.m-panel.m-panel-warning{border-top:3px solid #E79F3C;}
.m-panel.m-panel-primary{border-top:3px solid #3c8dbc;}
.m-panel.m-panel-info{border-top:3px solid #00c0ef;}
.m-panel.m-panel-cisco-blue{border-top:3px solid #085071;}

/* ITEM  */
.item-card ul.card-action-buttons{margin: -26px 10px 0 0; text-align: right;}
.item-card ul.card-action-buttons li{display: inline-block; padding-left: 5px;}
.item .card .card-content{padding: 24px;}
.item ul.card-action-buttons{margin: -26px 10px 0 0; text-align: right;}
.item ul.card-action-buttons li{display: inline-block; padding-left: 5px;}
.card ul.card-action-buttons{margin: -26px 10px 0 0; text-align: right;}
.card ul.card-action-buttons li{display: inline-block; padding-left: 5px;}
.card .btn-price{width:55px;height:55px;font-weight:600;font-size:1.0rem;line-height:58px;margin:10px;position:absolute;top:0;letter-spacing:0;}

/* PRICING */
.plans-container .collection{border: none;}
.plans-container .collection .collection-item{border-bottom:none;text-align:center;font-size:1.07rem;line-height:1.6em;}
.plans-container .collection .collection-item:before{font-family:"Material Icons";content:"check";font-size:1.28rem;line-height:1.6em;color:#00bfa5;font-style:normal;font-weight:normal;font-variant:normal;text-transform:none;-webkit-font-smoothing:antialiased;margin-right:10px;}
.plans-container .card .card-image{position:relative;width:100%;}
.plans-container .card .card-image .card-title {position:relative;font-size:1.28rem;line-height:1.6em;text-align:center;width:100%;padding:10px 15px;text-transform:uppercase;background:rgba(0,0,0,0.1);}
.plans-container .card .card-image .price{position:relative;font-size:5rem;line-height:1.6em;color:#fff;font-weight:300;text-align:center;}
.plans-container .card .card-image .price sup{font-weight:100;font-size:1.42rem;line-height:1.6em;top:-35px;}
.plans-container .card .card-image .price sub{font-weight:100;font-size:1.42rem;line-height:1.6em;top:0;}
.plans-container .card .card-image .price-desc{text-align:center;color:#fff;padding-bottom:10px;}
.plans-container .card .card-content{padding:0;}

/* CARD PANELS - CORNER LABELS */
.card-panel.alert{border:1px solid #F7BCD0;}
.card-panel.info{border:1px solid #3EA1E4;}
.card-panel.warning{border:1px solid #FDAD2A;}
.card-panel.success{border:1px solid #50AE54;}
.card-panel .card-panel-label{border-color: transparent #8a8a8a transparent transparent;border-color: rgba(255, 255, 255, 0) #8a8a8a rgba(255, 255, 255, 0) rgba(255, 255, 255, 0);border-style: solid;border-width: 0 4.375rem 2.5rem 0;float: right;height: 0px;width: 0px;margin: -25px;padding: 0px;-webkit-transform: rotate(360deg);-ms-transform: rotate(360deg);transform: rotate(360deg);}
.card-panel-content{padding-top:-20px;margin-top:-20px;}
.card-panel .card-panel-label-text{color:#fefefe;font-size:0.75rem;font-weight:bold;position:relative;right:-2.5rem;top:2px;white-space: nowrap;-webkit-transform:rotate(30deg);-ms-transform:rotate(30deg);transform:rotate(30deg);}
.card-panel.warning .card-panel-label{border-color: transparent #ffae00 transparent transparent;border-color: rgba(255, 255, 255, 0) #ffae00 rgba(255, 255, 255, 0) rgba(255, 255, 255, 0);}
.card-panel.success .card-panel-label{border-color: transparent #3adb76 transparent transparent;border-color: rgba(255, 255, 255, 0) #3adb76 rgba(255, 255, 255, 0) rgba(255, 255, 255, 0);}
.card-panel.alert .card-panel-label{border-color: transparent #cc4b37 transparent transparent;border-color: rgba(255, 255, 255, 0) #cc4b37 rgba(255, 255, 255, 0) rgba(255, 255, 255, 0);}
.card-panel.info .card-panel-label{border-color: transparent #37a0e6 transparent transparent;border-color: rgba(255, 255, 255, 0) #37a0e6 rgba(255, 255, 255, 0) rgba(255, 255, 255, 0);}
.card-panel-title{margin-top:-10px;}
.card .header2{background-color:#F7F7F7;padding:15px;padding-left:25px;border-bottom:1px solid rgba(204,204,204,0.35);font-size:18px;}

/* CARD */
.card .header{color:#555;padding:20px;position:relative;border-bottom:1px solid rgba(204,204,204,0.35)}
.card .header .header-dropdown{position:absolute;top:20px;right:15px;list-style:none}
.card .header .header-dropdown .dropdown-menu li{display:block!important}
.card .header .header-dropdown li{display:inline-block}
.card .header .header-dropdown i{font-size:20px;color:#999;-moz-transition:all .5s;-o-transition:all .5s;-webkit-transition:all .5s;transition:all .5s}
.card .header .header-dropdown i:hover{color:#000}
.card .header h2{margin:0;font-size:18px;font-weight:400;color:#fff}
.card .header h2 small{display:block;font-size:12px;margin-top:5px;color:#fff;line-height:15px}
.card .header h2 small a{font-weight:700;color:#777}
.card .header .col-xs-12 h2{margin-top:5px}

/* USING A CARD AS ALERT */
.card .c-alert-margin-color{width:10px;min-height: 10px;padding-top: 1px;}
.card .c-alert-margin-color-thin{width:5px;min-height: 10px;padding-top: 1px;}
.card .c-alert-margin-color-thick{width:20px;min-height: 10px;padding-top: 1px;}
.c-alert-content{padding-left:10px !important;}
.c-alert-danger{background-color: #D34135;}
.c-alert-content.c-alert-danger{background-color: #FAE4E3;padding:0;height:100%;width:100%;}
.c-alert-info{background-color: #4F96D1;}
.c-alert-content.c-alert-info{background-color: #DEF1FE;padding:0;height:100%;width:100%;}
.c-alert-warning{background-color: #F6C85E;}
.c-alert-content.c-alert-warning{background-color: #FDF6E4;padding:0;height:100%;width:100%;}
.c-alert-success{background-color: #6DBF83;}
.c-alert-content.c-alert-success{background-color: #E8FAE8;padding:0;height:100%;width:100%;}

/* ARROW BOX */
.arrow-left,.arrow-right,.arrow-top,.arrow-bottom{width:200px;height:30px;background-color:#ffc728;position:relative;top:3px;left:230px;padding:10px;line-height:22px;color:#313333;text-align:center}
.arrow-left:after,.arrow-right:after,.arrow-top:after,.arrow-bottom:after{content:'';width:0;height:0;border:10px solid transparent;position:absolute}
.arrow-left:after{border-right:10px solid #ffc728;margin-top:-10px;right:100%;top:50%}
.arrow-right:after{border-left:10px solid #ffc728;margin-top:-10px;left:100%;top:50%}
.arrow-top:after{border-bottom:10px solid #ffc728;margin-left:-10px;left:50%;bottom:100%}
.arrow-bottom:after{border-top:10px solid #ffc728;margin-right:-10px;right:50%;top:100%}

/* WELL */
.well{min-height:20px;padding:19px;margin-bottom:20px;background-color:#fafafb;border:1px solid #e6e6eb;border-radius:3px;-webkit-box-shadow:inset 0 1px 1px rgba(0,0,0,0.05);box-shadow:inset 0 1px 1px rgba(0,0,0,0.05)}

/* INFORMATION LABELS */
.info-label{display:inline;padding:.2em .6em .3em;font-size:70%;font-weight:700;line-height:1;color:#fff;text-align:center;white-space:nowrap;vertical-align:baseline;border-radius:.25em}

/* RIBBON */
.ribbon-box{position:relative;}
.ribbon{font-size:1rem;color:#fff;position:relative;width:7.5em;height:7.5em;overflow:hidden;display:block;position: relative;}
.ribbon_band{right:100%;left:auto;margin-right:-3.75em;margin-left:0;-webkit-transform-origin:top right;transform-origin:top right;-webkit-transform:rotate(-45deg);transform:rotate(-45deg);width:5.303301em;background-color:#9b9b9d;position:absolute;top:0;height:1.875em;text-align:center;user-select:none;display:block}
.ribbon_band::before{background-color:#9b9b9d;left:100%}
.ribbon_band::after,.ribbon_band::before{top:0;height:100%;width:10000px;margin:0 -1px;display:block;content:"";position:absolute}
.ribbon_band::after{background-color:#9b9b9d;right:100%}
.ribbon_band_content{position:absolute;top:50%;left:50%;-webkit-transform:translate(-50%,-50%);transform:translate(-50%,-50%);z-index:1;white-space:nowrap;overflow:hidden;display:block}
.ribbon_band_content::before{content:"";background-color:#9b9b9d;position:absolute;top:-1px;bottom:-1px;left:-1px;right:-1px;z-index:-1;overflow:hidden}
.ribbon.ribbon-right>.ribbon_band{left:100%;right:auto;margin-left:-3.75em;margin-right:0;-webkit-transform-origin:top left;transform-origin:top left;-webkit-transform:rotate(45deg);transform:rotate(45deg)}
.ribbon_content{margin-left:0px;padding-left:0px;height:100%;width:100%;left:0px;top:0px;position:absolute;}

/* RIBBON COLORS */
.ribbon.ribbon-danger>.ribbon_band{background-color:#D34135;}
.ribbon.ribbon-danger>.ribbon_band>.ribbon_band_content::before{background-color:#D34135;}
.ribbon.ribbon-danger>.ribbon_band::before{background-color:#D34135;}
.ribbon.ribbon-danger>.ribbon_band::after{background-color:#D34135;}
.ribbon.ribbon-success>.ribbon_band{background-color:#6DBF83;}
.ribbon.ribbon-success>.ribbon_band>.ribbon_band_content::before{background-color:#6DBF83;}
.ribbon.ribbon-success>.ribbon_band::before{background-color:#6DBF83;}
.ribbon.ribbon-success>.ribbon_band::after{background-color:#6DBF83;}

/* BAR GRAPH */
.bar-graph{padding:0;width:100%;display:-ms-flexbox;display:flex;-ms-flex-align:end;align-items:flex-end;height:425px;margin:0}
.bar-graph li{display:block;padding:1.5625rem 0;position:relative;text-align:center;vertical-align:bottom;border-radius:4px 4px 0 0;max-width:20%;height:100%;margin:0 1.8% 0 0;-ms-flex:1 1 15%;flex:1 1 15%}
.bar-graph .bar-graph-axis{-ms-flex:1 1 8%;flex:1 1 8%;display:-ms-flexbox;display:flex;-ms-flex-direction:column;flex-direction:column;-ms-flex-pack:justify;justify-content:space-between}
.bar-graph .bar-graph-label{margin:0;background-color:none;color:#8a8a8a;position:relative}
@media print,screen and (min-width:40em){.bar-graph .bar-graph-label:after,.bar-graph .bar-graph-label:before{content:"";position:absolute;border-bottom:1px dashed #8a8a8a;top:0;left:0;height:50%;width:20%}}
@media print,screen and (min-width:40em) and (min-width:64em){.bar-graph .bar-graph-label:after,.bar-graph .bar-graph-label:before{width:30%}}
@media print,screen and (min-width:40em){.bar-graph .bar-graph-label:after{left:auto;right:0}}
@media print,screen and (min-width:40em){.bar-graph .percent{font-size:3.875rem}}
.bar-graph .percent{letter-spacing:-3px;opacity:.4;width:100%;font-size:1.875rem;position:absolute}
.bar-graph .percent span{font-size:1.875rem}
.bar-graph .description{font-weight:800;opacity:.5;text-transform:uppercase;width:100%;font-size:14px;bottom:20px;position:absolute;font-size:1rem;overflow:hidden}
.bar-graph .bar.primary{border:1px solid #1779ba;background:linear-gradient(#2196e3,#1779ba 70%)}
.bar-graph .bar.secondary{border:1px solid #767676;background:linear-gradient(#909090,#767676 70%)}
.bar-graph .bar.success{border:1px solid #3adb76;background:linear-gradient(#65e394,#3adb76 70%)}
.bar-graph .bar.warning{border:1px solid #ffae00;background:linear-gradient(#ffbe33,#ffae00 70%)}
.bar-graph .bar.alert{border:1px solid #cc4b37;background:linear-gradient(#d67060,#cc4b37 70%)}

